public class passing_array_as_argument {
    public static void update(int marks[],int nochangeable){
        for (int i = 0; i < marks.length; i++) {
            marks[i]=marks[i]+1;
            nochangeable=5;
        }

    }

    public static void main(String[] args) {
       int  nochangeable=10;
      int marks[]={99,98,94};
      update(marks,nochangeable);
      for(int i=0;i<marks.length;i++){
          System.out.print(marks[i]+" ");

      }
        System.out.println();
    }
}
