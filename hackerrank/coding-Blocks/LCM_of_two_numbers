import java.util.*;
public class Main {
    public static void main(String args[]) {
		Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int max,k;
        if(a>b)
         	max=a;
        else
            max=b;
        for(int i=max;;i++)
        {
            if(i%a==0 && i%b==0)
            {
            k=i;
            break;
            }
        }
        System.out.print(k);
         

    }
}
