import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
    int n1=sc.nextInt();
    int n2=sc.nextInt();
    int c=0;
    for(int i=1;c<n1;i++)
    {
        int k=3*i+2;
        if(k%n2==0)
        continue;
        else
        {
            System.out.println(k);
            c++;
        }
    }
    }
}
