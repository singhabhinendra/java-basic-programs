import java.util.Scanner;

public class multiplication_table {
    public static void main(String[]args) {
        Scanner sc = new Scanner(System.in);
        int p = sc.nextInt();
        int i;
        for (i = 1; i < 10; i++) {
            System.out.println(i +"X"+p +"="+p*i);
        }
    }
}
