import java.util.Scanner;

public class oddnatural {
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int sum=0,i;
        for(i=0;i<=n;i++){
            if(i%2!=0){
                sum=sum+i;}
        }
        System.out.println(sum);
    }
}

